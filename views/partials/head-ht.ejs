<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<style>
    body {
    background-color: rgb(0, 0, 0);
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    position: fixed;
}

.border {
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 1000;
    pointer-events: none;
}

.profile {
    position: fixed;
    top: 0;
    left: 0;
    width: 200px;
    height: 200px;
    border-radius: 100px;
    border: none;
    background-image: url("partials/images/profile.png");
    background-color: rgba(0, 0, 0, 0);
    z-index: 3;
    transition-duration: 1s;
    -webkit-animation: proload 2s 1;
    animation-iteration-count: 1;
    pointer-events: none;
}

@-webkit-keyframes proload {
    0% {
        transform: rotate(0deg);
    }
    50% {
        transform: rotate(360deg);
    }
    100% {
        transform: rotate(360deg);
    }
}

.name {
    color: rgb(200, 200, 255);
    font-size: 22px;
    width: 0px;
    padding-left: 50px;
    height: 29px;
    background-color: rgb(25, 25, 25);
    border-right: solid 3px rgb(0, 0, 255);
    position: fixed;
    left: 150px;
    top: 20px;
    z-index: 0;
}

.email {
    color: rgb(220, 220, 255);
    font-size: 12px;
    width: 0px;
    padding-left: 55px;
    height: 20px;
    background-color: rgb(25, 25, 25);
    border-right: solid 3px rgb(0, 0, 255);
    position: fixed;
    left: 150px;
    top: 51px;
    z-index: 0;
}


.phone {
    color: rgb(220, 220, 255);
    font-size: 12px;
    width: 0px;
    padding-left: 60px;
    height: 20px;
    background-color: rgb(25, 25, 25);
    border-right: solid 3px rgb(0, 0, 255);
    position: fixed;
    left: 150px;
    top: 73px;
    z-index: 0;
}


.github {
    color: rgb(220, 220, 255);
    font-size: 12px;
    width: 0px;
    padding-left: 60px;
    height: 20px;
    background-color: rgb(25, 25, 25);
    border-right: solid 3px rgb(0, 0, 255);
    position: fixed;
    left: 150px;
    top: 95px;
    z-index: 0;
}

.menu:hover {
    width: 600px;
}

.launcher {
    display: none;
    margin-top: -5px;
    font-size: 12px;
    background-repeat: no-repeat;
    padding-top: 15px;
    position: relative;
    color: rgb(200, 200, 255);
    width: 137px;
    height: 48px;
    text-align: center;
    border: none;
    background-color: rgba(255, 255, 255, 0);
    background-image: url("partials/images/normal.png");
    outline: none;
    transition-duration: 0.5s;
}

.launcher:hover {
    padding-top: 20px;
    width: 148px;
    height: 56px;
    background-image: url("partials/images/hover.png");
}

.launcher:active {
    padding-top: 20px;
    width: 147px;
    height: 56px;
    background-image: url("partials/images/click.png");
}

.dp {
    width: 73px;
    height: 73px;
    border-radius: 50%;
    margin-left: 64px;
    margin-top: 64px;
    -webkit-animation: dpl 10s 1;
    animation-iteration-count: infinite;
}

@-webkit-keyframes dpl {
    0% {
        transform: rotate3d(0, 1, 0, 0deg);
    }
    100% {
        transform: rotate3d(0, 1, 0, 360deg);
    }
}

.profile-holder {
    position: fixed;
    background-color: rgb(0, 0, 0);
    width: 200px;
    height: 200px;
    border-radius: 100px;
    top: 0;
    left: 0;
    z-index: 2;
}

#name {
    display: none;
}

#email {
    display: none;
}

#phone {
    display: none;
}

#git {
    display: none;
}

.btn-group {
    list-style-type: none;
}

.navr {
    position: fixed;
    top: 200px;
    left: 100px;
    z-index: 999;
}

.cover {
    position: fixed;
    height: 150px;
    width: 99%;
    top: 0;
    border-bottom: solid 1px rgb(25, 25, 25);
    background-color: rgb(0, 0, 0);
    z-index: 1;
}

.content {
    margin-top: 150px;
    position: fixed;
    overflow: auto;
    scroll-behavior: smooth;
    width: 970px;
    height: 430px;
    left: 300;
    padding-top: 30px;
    padding-left: 50px;
    z-index: 0;
}

.content::-webkit-scrollbar {
    display: none;
    width: 0 !important;
}

.dat {
    position: relative;
    margin-top: 10px;
    padding: 20px 20px 20px 20px;
    width: 900px;
    border-top: solid 3px rgb(0, 0, 255);
    border-left: solid 1px rgb(100, 100, 100);
    border-right: solid 1px rgb(100, 100, 100);
    border-bottom: solid 1px rgb(100, 100, 100);
    -webkit-animation: cnl 1s 1;
    animation-iteration-count: 1;
}

.heading {
    font-size: 20px;
    color: rgb(150, 150, 255);
    -webkit-animation: hdl 2s 1;
    animation-iteration-count: 1;
}

.conte {
    margin-left: 10px;
    color: rgb(225, 225, 255);
    font-size: 15px;
    -webkit-animation: ctl 2s 1;
    animation-iteration-count: 1;
}

th {
    padding: 5px 10px 8px 10px;
    border-left: solid 3px rgb(0, 0, 255);
    color: rgb(225, 225, 255);
    font-size: 15px;
    background-color: rgb(25, 25, 25);
}

td {
    padding: 5px 10px 8px 10px;
    border-left: solid 1px rgb(100, 100, 100);
    color: rgb(225, 225, 255);
    font-size: 15px;
    background-color: rgb(25, 25, 25);
}

.subh {
    color: rgb(175, 175, 255);
}

dd {
    color: rgb(225, 225, 255);
}

@-webkit-keyframes cnl {
    0% {
        transform: translateX(-1000px);
    }
    90% {
        transform: translateX(5px);
    }
    100% {
        transform: translateX(0px);
    }
}

@-webkit-keyframes hdl {
    0% {
        transform: translateX(10000px);
    }
    90% {
        transform: translateX(-5px);
    }
    100% {
        transform: translateX(0px);
    }
}

@-webkit-keyframes ctl {
    0% {
        transform: translateX(10000px);
    }
    90% {
        transform: translateX(-5px);
    }
    100% {
        transform: translateX(0px);
    }
}

a {
    text-decoration: none;
}
</style>
<script>
    
function shcont() {
    var x = document.getElementById("namec");
    var y = document.getElementById("emailc");
    var z = document.getElementById("phonec");
    var a = document.getElementById("githubc");
    var b = document.getElementById("name");
    var c = document.getElementById("email");
    var d = document.getElementById("phone");
    var e = document.getElementById("git");
    var f = document.getElementById("edu");
    var h = document.getElementById("skill");
    var m = document.getElementById("project");
    var n = document.getElementById("psa");
    var o = document.getElementById("tq");
    var width = 0;
    var i = setInterval(animx, 1);
    function animx() {
        if (width == 400) {
            clearInterval(i);
        } else {
            width += 5;
            x.style.width = width + 'px';
        }
    }
    var widthj = 0;
    var j = setInterval(animy, 1);
    function animy() {
        if (widthj == 350) {
            clearInterval(j);
        } else {
            widthj += 5;
            y.style.width = widthj + 'px';
        }
    }
    var widthk = 0;
    var k = setInterval(animz, 1);
    function animz() {
        if (widthk == 300) {
            clearInterval(k);
        } else {
            widthk += 5;
            z.style.width = widthk + 'px';
        }
    }
    var widthl = 0;
    var l = setInterval(anima, 1);
    function anima() {
        if (widthl == 250) {
            clearInterval(l);
        } else {
            widthl += 5;
            a.style.width = widthl + 'px';
        }
    }
    setTimeout(function () {
        b.style.display = "block";
        setTimeout(function () {
            c.style.display = "block";
            setTimeout(function () {
                d.style.display = "block";
                setTimeout(function () {
                    e.style.display = "block";
                }, 100);
            }, 100);
        }, 100);
    }, 100);
    setTimeout(function () {
        f.style.display = "block";
        setTimeout(function () {
            o.style.display = "block";
            setTimeout(function () {
                h.style.display = "block";
                setTimeout(function () {
                    m.style.display = "block";
                    setTimeout(function () {
                        n.style.display = "block";
                    }, 100);
                }, 100);
            }, 100);
        }, 100);
    }, 100);
}

function rotor() {
    document.getElementById("rotor").style.transform = "rotate(360deg)";
}
</script>