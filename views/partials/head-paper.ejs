<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<style>
    body {
    background-color: rgb(200, 200, 200);
}

.container2 {
    margin-top: 0px;
    margin-left: -50px;
}

.header {
    top: 0;
    left: 0;
    width: 100%;
    height: 200px;
    margin: auto;
    padding-top: 20px;
    padding-left: 10px;
    padding-bottom: 5px;
    background-color: rgb(0, 0, 255);
    color: rgb(255, 255, 255);
    font-family: Segoe UI;
    box-shadow: 0 10px 20px 0 rgb(0, 0, 0);
}

.name {
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    color: rgb(255, 255, 255);
    font-size: 50px;
    float: bottom;
}

.cont {
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    font-size: 15px;
    color: rgb(255, 255, 255);
    float: bottom;
}

.dp {
    position: absolute;
    width: 100px;
    height: 100px;
    border-radius: 50%;
    border: solid 1px rgb(255, 255, 255);
    float: right;
    top: 55px;
    right: 20px;
}

.navbtn {
    display: none;
    margin-top: 20px;
    height: 60px;
    width: 200px;
    font-size: 15px;
    text-align: center;
    padding-top: 20px;
    background-color: rgb(255, 255, 255);
    border: none;
    border-radius: 2px;
    color: rgb(100, 100, 100);
    box-shadow: 10px 10px 20px 0 rgb(0, 0, 0);
    transition-duration: 0.3s;
}

.navbtn:hover {
    background-color: rgb(255, 0, 0);
    color: rgb(255, 255, 255);
}

.navbtn:active {
    background-color: rgb(0, 0, 255);
    color: rgb(255, 255, 255);
}

a {
    text-decoration: none;
}

a:hover {
    text-decoration: none;
}

.content {
    margin-top: 15px;
    overflow: auto;
    scroll-behavior: smooth;
    width: 100%;
    height: 65%;
    left: 200px;
    padding-left: 50px;
}

.content::-webkit-scrollbar {
    display: none;
    width: 0 !important;
}

.dat {
    position: relative;
    margin-top: 10px;
    padding: 20px 20px 20px 20px;
    width: 100%;
    overflow: auto;
    background-color: rgb(255, 255, 255);
    border: none;
    border-radius: 5px;
    border-top: solid 3px rgb(0, 0, 255);
    border-left: solid 1px rgb(100, 100, 100);
    border-right: solid 1px rgb(100, 100, 100);
    border-bottom: solid 1px rgb(100, 100, 100);
    -webkit-animation: cnl 1s 1;
    animation-iteration-count: 1;
}

.heading {
    font-size: 20px;
    color: rgb(100, 100, 255);
    -webkit-animation: hdl 2s 1;
    animation-iteration-count: 1;
}

.conte {
    margin-left: 10px;
    color: rgb(100, 100, 100);
    font-size: 15px;
    -webkit-animation: ctl 2s 1;
    animation-iteration-count: 1;
}

th {
    padding: 5px 10px 8px 10px;
    border-left: solid 3px rgb(0, 0, 255);
    border-bottom: solid 1px rgb(100, 100, 100);
    color: rgb(100, 100, 100);
    font-size: 15px;
    background-color: rgb(200, 200, 200);
}

td {
    padding: 5px 10px 8px 10px;
    border-left: solid 1px rgb(100, 100, 100);
    border-bottom: solid 1px rgb(100, 100, 100);
    color: rgb(100, 100, 100);
    font-size: 15px;
    background-color: rgb(225, 225, 225);
}

.subh {
    color: rgb(150, 150, 255);
}

dd {
    color: rgb(100, 100, 100);
}
@-webkit-keyframes cnl {
    0% {
        transform: translateX(-1000px);
    }
    90% {
        transform: translateX(5px);
    }
    100% {
        transform: translateX(0px);
    }
}

@-webkit-keyframes hdl {
    0% {
        transform: translateX(10000px);
    }
    90% {
        transform: translateX(-5px);
    }
    100% {
        transform: translateX(0px);
    }
}

@-webkit-keyframes ctl {
    0% {
        transform: translateX(10000px);
    }
    90% {
        transform: translateX(-5px);
    }
    100% {
        transform: translateX(0px);
    }
}
</style>
<script>
    function load() {
    var f = document.getElementById("edu");
    var h = document.getElementById("skill");
    var m = document.getElementById("project");
    var n = document.getElementById("psa");
    var o = document.getElementById("tq");
    setTimeout(function () {
        f.style.display = "block";
        setTimeout(function () {
            o.style.display = "block";
            setTimeout(function () {
                h.style.display = "block";
                setTimeout(function () {
                    m.style.display = "block";
                    setTimeout(function () {
                        n.style.display = "block";
                    }, 100);
                }, 100);
            }, 100);
        }, 100);
    }, 100);
}
</script>